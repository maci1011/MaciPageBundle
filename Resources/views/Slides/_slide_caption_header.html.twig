
{% if item.title is not null %}
<div class="slide-header-wrapper">
<header class="slide-header">
	{% if item.link %}
	<a href="{{ item.link }}" alt="{{ item.title }}">
		<h3 class="slide-title">{{ item.title }}</h3>
	</a>
	{% else %}
	<h3>{{ item.title }}</h3>
	{% endif %}
</header>
</div>
{% endif %}
